  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@lezer/generator": "^1.0.0",
    "@lezer/lr": "^1.0.0",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "babel-jest": "^29.6.1",
    "eslint": "^8.44.0",
    "jest": "^29.6.1",
    "lint-staged": "^16.1.2",
    "prettier": "^3.0.0",
    "rollup": "^2.79.2",
    "rollup-plugin-combine": "2.1.1",
    "rollup-plugin-match": "1.0.0"
  },
  "lint-staged": {
    "**/*.{js,cjs,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,md}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "@lezer/lr": "^1.0.0"
